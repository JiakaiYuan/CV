    <script>
        new Vue({
            // eslint-disable-line
            el: "#myCV",
            data: {
                experienceDisplay: true,
                educationDisplay: false,
                workexperienceDisplay: false,
                mainDisplay: true,
                webProjectDisplay: false,
                javascriptDisplay: false,
                phpDisplay: false,
                projectDisplay: false,
                KimH: 'email: kim.huang@ecommistry.com ',
				MikeL: 'email: Michael.lance@ara.ac.nz '
				
            },
            methods: {
                resetDiaplay: function () {
                    this.mainDisplay = true;
                    this.projectDisplay = false;
                },
                displayWebProject: function () {
                    this.mainDisplay = false;
                    this.projectDisplay = true;
                    this.webProjectDisplay = true;
                    this.javascriptDisplay = false;
                    this.phpDisplay = false;
                },
                displayJSProject: function () {
                    this.mainDisplay = false;
                    this.projectDisplay = true;
                    this.webProjectDisplay = false;
                    this.javascriptDisplay = true;
                    this.phpDisplay = false;
                },
                displayPHPProject: function () {
                    this.mainDisplay = false;
                    this.projectDisplay = true;
                    this.webProjectDisplay = false;
                    this.javascriptDisplay = false;
                    this.phpDisplay = true;
                }
            },
            created() {

            }
        });

